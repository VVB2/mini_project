{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "react/jsx-filename-extension": "off",
    "no-restricted-syntax": "off",
    "guard-for-in": "off",
    "react/prop-types": "off",
    "react/no-array-index-key": "off"
  }
}
